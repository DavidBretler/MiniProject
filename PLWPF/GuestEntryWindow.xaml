        <Button x:Name="exsiting_Guest_Request_button_" Content=" existing request" HorizontalAlignment="Left" Margin="235,82,0,0" VerticalAlignment="Top" FontFamily="David" Click="Button_Click_1"/>
        <Button x:Name="___NewOrderButton_" Content="new request" HorizontalAlignment="Left" Margin="59,82,0,0" VerticalAlignment="Top"   FontFamily="David" Click="___NewOrderButton__Click"/>
